              "data-toggle": "modal",
              "data-target": "#metadataACM",
              "onclick":"web"+i+"()"
            })
        }
        else
        {
          $("tbody tr td lu li:eq("+i+")").css("list-style","none")//quita el punto de la lista
          $("tbody tr td lu li:eq("+i+") button").attr("id","btn"+i)

            .attr({
              "class": "btn btn-primary",
              "disabled":true,
              "onclick":"web"+i+"()"
            })
        }
      }
    }
  });   //end jquery
